class Employee(object):
    """Models real-life employees!"""
    def init(self, employeename):
        self.employeename = employee_name

    def calculate_wage(self, hours):
        self.hours = hours
        return hours * 20.00

class PartTimeEmployee(Employee):

    def calculate_wage(self,hours):
        self.hours = hours
        return hours * 12.00